@model IEnumerable<Garage_ManagementFinal.order_stkitem>

@{
    ViewBag.Title = "ViewAccepteditem";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>ViewAccepteditem</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="card-body ">
    <table id="datatablesSimple">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Stock.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.quantity)
                </th>
            
                <th>
                    @Html.DisplayNameFor(model => model.Alpha)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Entry_Form.plate_no)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.date)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Orderedby)
                </th>


                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Stock.name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.quantity)
                    </td>
                  
                    <td>
                        @Html.DisplayFor(modelItem => item.Alpha)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Entry_Form.plate_no)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Orderedby)
                    </td>


                    <td>
                    </td>
                </tr>
            }
        </tbody>
    </table>

    <script>
    $(document).ready(function () {
        $('#datatablesSimple').DataTable({
            "ordering": false
        });

    });
    </script>

    </div>