@model IEnumerable<Garage_ManagementFinal.order_stkitem>

@{
    ViewBag.Title = "allview";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<h2>See All Orders</h2>


<div class="card-body ">
    <table id="datatablesSimple">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.quantity)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Entry_Form.region)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Stock.name)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.quantity)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Entry_Form.region)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Stock.name)
                    </td>
                    <td>
                    </td>
                </tr>
            }

        </tbody>
    </table>

    <script>
        $(document).ready(function () {
            $('#datatablesSimple').DataTable({
                "ordering": false
            });

        });
    </script>
